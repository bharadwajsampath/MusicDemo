@model IEnumerable<MusicApp.ViewModels.ArtistAlbums>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
</p>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.albumname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.artistname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.genre)
        </th>
       
       
      
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.albumname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.artistname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.genre)
        </td>
       
       
     
        <td>
            @Html.ActionLink("View Tracks", "Tracks", new {  id=item.albumid  }) |
          
        </td>
    </tr>
}

</table>
